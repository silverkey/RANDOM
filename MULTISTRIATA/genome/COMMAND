nohup exonerate -q /media/LOCAL_DATA/DNAseq_SZN/multistriata/transcriptome/Pseudo-Nitzschia_multistriata_final_contigs.fa -t /media/LOCAL_DATA/DNAseq_SZN/multistriata/genome/Assembly_abyss_v1.0/v1.0_over_100000.fasta -m est2genome --showquerygff yes --showtargetgff yes --bestn 5 --dpmemory 10000 --fsmmemory 10000 --maxintron 50000 > exonerate_genome1.0_100000.out &

nohup exonerate -q /media/LOCAL_DATA/DNAseq_SZN/multistriata/transcriptome/Pseudo-Nitzschia_multistriata_final_contigs.fa -t /media/LOCAL_DATA/DNAseq_SZN/multistriata/genome/Assembly_abyss_v1.0/v1.0_over_100000.fasta -m est2genome --showtargetgff yes --bestn 5 --dpmemory 10000 --fsmmemory 10000 --maxintron 10000 --geneseed 200 --percent 80 > exonerate_genome1.0_100000_200_80.out &

nohup exonerate -q /media/LOCAL_DATA/DNAseq_SZN/multistriata/transcriptome/Pseudo-Nitzschia_multistriata_final_contigs.fa -t /media/LOCAL_DATA/DNAseq_SZN/multistriata/genome/Assembly_abyss_v1.0/v1.0_over_100000.fasta -m est2genome --showtargetgff yes --bestn 5 --dpmemory 10000 --fsmmemory 10000 --maxintron 10000 --geneseed 200 --percent 90 > exonerate_genome1.0_100000_200_90.out &

nohup exonerate -q /media/LOCAL_DATA/DNAseq_SZN/multistriata/transcriptome/Pseudo-Nitzschia_multistriata_final_contigs.fa -t /media/LOCAL_DATA/DNAseq_SZN/multistriata/genome/Assembly_abyss_v1.0/v1.0_over_100000.fasta -m est2genome --showtargetgff yes --bestn 5 --dpmemory 10000 --fsmmemory 10000 --maxintron 10000 --geneseed 200 --percent 80 --ryo ">%qi length=%ql alnlen=%qal\n>%ti length=%tl alnlen=%tal\n" > exonerate_genome1.0_100000_200_80_for_parsing.out &

nohup exonerate -q /media/LOCAL_DATA/DNAseq_SZN/multistriata/transcriptome/Pseudo-Nitzschia_multistriata_final_contigs.fa -t /media/LOCAL_DATA/DNAseq_SZN/multistriata/genome/Assembly_abyss_v1.0/v1.0_over_100000.fasta -m est2genome --showtargetgff yes --bestn 1 --dpmemory 10000 --fsmmemory 10000 --maxintron 10000 --geneseed 200 --percent 80 --ryo ">QUERY: %qi length=%ql alnlen=%qal\n>TARGET: %ti length=%tl alnlen=%tal\n>INFO: percentage=%pi identical=%ei mismatch=%em strand=%g\n>CIGAR: %C" > exonerate_genome1.0_100000_200_80_bestn1_for_parsing.out &

